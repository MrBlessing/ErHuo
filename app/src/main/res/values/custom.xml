<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Image_Text_text">
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="Image_Text_image">
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_margin">5dp</item>
    </style>
    
    
    <style name="Cutting_line">
        <item name="android:background">#bbe7e4</item>
    </style>

    <style name="list_container">
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingBottom">20dp</item>
    </style>

    <style name="popup_release_price_text">
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:textSize">20sp</item>
    </style>


    <style name="mainColor">
        <item name="android:background">#bbe7e4</item>
    </style>

    <style name="main_EditText">
        <item name="android:background">@null</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="main_Text">
        <item name="android:textSize">15sp</item>
    </style>

    <style name="up_background_Text" parent="main_Text">
        <item name="android:textColor">#fff</item>
    </style>


    <style name="main_Button">
        <item name="android:textSize">15sp</item>
        <item name="android:background">@drawable/corner_button</item>
        <item name="android:textColor">#fb7375</item>
    </style>

    <style name="main_statusBar" parent="mainColor">
        <item name="android:paddingTop">25dp</item>
    </style>

</resources>